#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;

vector<int> solution(vector<int> arr) 
{
    vector<int> answer;

    // [실행] 버튼을 누르면 출력 값을 볼 수 있습니다.
    
    //sort(arr.begin(), arr,end());
    arr.erase(unique(arr.begin(), arr.end()), arr.end());
    
    cout << "Hello Cpp" << endl;

    return arr;
}
