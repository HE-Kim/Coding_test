#include <string>
#include <vector>
#include <iostream>
using namespace std;

long long solution(int a, int b) {
    long long answer = 0;
    if(a>b)
    {
        int temp=b;
        b=a;
        a=temp;
    }
    cout<<a<<b;
        
        
    for(int i=a; i<=b; i++)
    {
        answer+=i;
    }
    return answer;
}
